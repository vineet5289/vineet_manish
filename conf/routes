# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                                   controllers.Application.dashBoard()
GET     /        	                        controllers.SRPController.index()
GET     /home                        		controllers.SRPController.home()
GET     /login                       		controllers.SRPController.preLogin()
POST    /login                       		controllers.SRPController.postLogin()
GET     /logout                       		controllers.SRPController.logout()
GET     /sendmail                           controllers.Application.sendMailTest()

# RegistrationRequest

GET     /addnewschoolrequest                	controllers.RegistrationRequest.preAddNewSchoolRequest()
GET     /registration/:reg_cat              	controllers.RegistrationHandler.preRegistor(reg_cat: String)
GET     /Register                            	controllers.RegistrationRequest.registerPage()
GET     /registrationRequest                	controllers.RegistrationRequest.preRegistrationRequest()
POST     /registrationRequest               	controllers.RegistrationRequest.postRegistrationRequest()
POST    /registration                			controllers.RegistrationHandler.registor()
GET     /addnewschoolrequest                    controllers.RegistrationRequest.postAddNewSchoolRequest()
GET     /otpLogin                               controllers.RegistrationRequest.otpLoginRequest()


# Map static resources from the /public folder to the /assets URL path
#GET     /assets/*file                           controllers.Assets.versioned(path="/public", file: Asset)
GET     /assets/*file                            controllers.Assets.at(path="/public", file: String)