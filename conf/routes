# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                                   controllers.Application.dashBoard()
GET     /        	                        controllers.SRPController.index()
GET     /home                        		controllers.SRPController.home()
GET     /login                       		controllers.SRPController.preLogin()
POST    /login                       		controllers.SRPController.postLogin()
GET     /logout                       		controllers.SRPController.logout()
GET     /sendmail                           controllers.Application.sendMailTest()

# RegistrationRequest

GET     /addnewschoolrequest                	controllers.RegistrationRequest.preAddNewSchoolRequest()
GET     /newUser                            	controllers.RegistrationRequest.newUser()
GET     /registrationRequest                	controllers.RegistrationRequest.registrationRequest()
POST    /registrationRequest               	controllers.RegistrationRequest.postRegistrationRequest()
POST    /addnewschoolrequest                    controllers.RegistrationRequest.postAddNewSchoolRequest()

GET     /registerschool                 controllers.RegistrationHandler.preRegistorSchool()
POST     /registerschool            	controllers.RegistrationHandler.postRegistorSchool()

GET     /registersemp                 controllers.RegistrationHandler.preRegistorSchool()
POST     /registeremp            	controllers.RegistrationHandler.postRegistorSchool()

# Map static resources from the /public folder to the /assets URL path
#GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /assets/*file                  controllers.Assets.at(path="/public", file: String)